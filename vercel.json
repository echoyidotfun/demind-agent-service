{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ],
  "crons": [
    {
      "path": "/api/v1/cron/defillama-all",
      "schedule": "0 1 * * *"
    },
    {
      "path": "/api/v1/cron/coingecko-all",
      "schedule": "0 3 * * *"
    }
  ]
}